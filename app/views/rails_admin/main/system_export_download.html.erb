<html>
<head>
<script type="text/javascript">
    window.setTimeout(function() {
        <%if @error%>
            alert("<%=@error-%>")
        <%else%>
            window.parent.$j('#system_export_build_frame').html('<iframe src=' + '<%="#{system_export_path(@model_name)}?id=#{@application.id}&mode=download&model_name=#{@model_name}"-%>' + '/>');
        <%end%>
        window.setTimeout(function() {
            window.parent.$j('div.createAssociatedRecordDialog').data("dialog").close();
        }, 1000);
    });
</script>
</head>
<body/>
</html>
