<div class="ui-widget-header">
  <%= month_names[month - 1] %> <%= year %> <%= t("admin.history.name").downcase %>
</div>
<div class="ra-block-content">
  <% if authorized? :see_history %>
    <table class="grid">
      <tbody>
        <% history.each do |t|%>
        <tr class="<%= cycle 'odd', 'even' %>">
           <td style="text-align: left;padding-right: 15px"><%= t.created_at.localtime  %></td> 
           <td style="text-align: left;padding-right: 15px"><%= t.try(:username)%></td>
           <td style="text-align: left;padding-right: 15px"><%= history_output(t) %></td>
           <td style="text-align: left;padding-right: 15px"><%= t.table %></td> 
        </tr>
        <% end %>
      </tbody>
    </table>
  <%end%>
</div>
