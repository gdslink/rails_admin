<% head_javascript "rails_admin/application.js" %>
      <div class="ra-block">
        <div class="ui-widget-header">
          <%= t('admin.delete.delete_confirmation')%>
        </div>
<%= render(:partial => 'layouts/rails_admin/flash', :locals => {:flash => flash}) -%>
        <div class="ra-block-content">
          <p><%=t('admin.delete.are_your_sure_you_want_to_delete_this_object_and_associations')%></p>
          <%= render :partial => "delete_notice", :collection => @bulk_objects %>
          <%= form_tag bulk_destroy_path(current_scope_parameters.merge(:model_name => @abstract_model.to_param, :bulk_ids => @bulk_objects.map(&:id))) do %>
            <ul class="submit clearfix">
              <li>
                <%= submit_tag t("admin.delete.confirmation"), :class => "ra-button ui-state-highlight" %>
              </li>
              <li>
                <%= submit_tag t("admin.new.cancel"), :class => "ra-button", :name => "_continue" %>
              </li>
            </ul>
          <% end =%>
        </div>
      </div>